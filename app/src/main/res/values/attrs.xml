<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="pingleEditText">
        <attr name="title" format="reference|string" />
        <attr name="text" format="reference|string" />
        <attr name="hint" format="reference|string" />
        <attr name="maxLength" format="integer" />
        <attr name="focusable" format="boolean" />
        <attr name="checkVisibility" format="enum">
            <enum name="visible" value="1" />
            <enum name="invisible" value="2" />
            <enum name="gone" value="3" />
        </attr>
        <attr name="copyVisibility" format="enum">
            <enum name="visible" value="1" />
            <enum name="invisible" value="2" />
            <enum name="gone" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="PingleSearch">
        <attr name="pingleSearchHint" format="reference|string" />
    </declare-styleable>
</resources>
